{
  "manifest_version": 3,
  "description": "Add website usage to google sheets",
  "name": "Sheet Tracker",
  "key": "",
  "version": "0.0.1",
  "background": {
    "service_worker": "./src/serviceWorker.ts"
  },
  "oauth2": {
    "client_id": "132127179083-vomcu69kup22ntjil28ljcsdti66ru6b.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/spreadsheets",
      "https://www.googleapis.com/auth/drive"
    ]
  },
  "host_permissions": ["<all_urls>"],
  "permissions": ["scripting", "storage", "activeTab", "identity", "sidePanel"],

  "side_panel": {
    "default_path": "index.html"
  }
}
